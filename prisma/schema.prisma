// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ticket {
  id          String  @id @default(uuid())
  stripeId    String  @unique
  displayName String
  description String
  price       Int
  groupName   String
  isActive    Boolean
}
